@prefix schema: <http://schema.org/> .

<https://example.com/John-Doe> a schema:Person ;
	schema:age 18 ;
	schema:gender "female" ;
	schema:address <https://example.com/John-Does-Address> ;
	schema:givenName "John", "Johnny" .

<https://example.com/John-Does-Address> a <http://schema.org/AddressShape> ;
	schema:postalCode 555 ;
    schema:streetAddress "New York Place 1" .